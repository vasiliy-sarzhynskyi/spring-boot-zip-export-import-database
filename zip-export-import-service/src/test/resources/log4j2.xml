<?xml version="1.0" encoding="UTF-8"?>
<Configuration name="ZipExportImportDataService" monitorInterval="30">
    <Appenders>
        <Console name="STDOUT" target="SYSTEM_OUT">
            <PatternLayout pattern="%d %-5p [%t] %C{4} (%F:%L) - %m%n"/>
        </Console>
    </Appenders>

    <Loggers>
        <Logger name="com.vsarzhynskyi.export_import_db" level="DEBUG"/>
        <Logger name="org.hibernate" level="DEBUG"/>
        <Root level="INFO">
            <AppenderRef ref="STDOUT"/>
        </Root>
    </Loggers>
</Configuration>